<Theme 
    themeId="xmlui-orange"
    color-text-H1="blue"
>
    <App>
        <Button
      variant="ghost"
      themeColor="secondary"
      icon="{activeThemeTone === 'light' ? 'moon' : 'sun'}"
      onClick="setThemeTone(activeThemeTone === 'light' ? 'dark' : 'light')"
    />

        <NavPanel>
            <NavLink label="Home" to="/" />
        </NavPanel>
        <Pages>
            <Page url="/">
                <Home />
            </Page>
        </Pages>
    </App>
</Theme>